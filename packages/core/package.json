{
  "name": "@react-native-camera/core",
  "version": "0.1.0",
  "description": "Camera component for React Native",
  "main": "lib/index",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-camera-core.podspec",
    "!android/build",
    "!ios/build"
  ],
  "scripts": {
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet"
  },
  "keywords": [
    "react",
    "react-native",
    "ios",
    "android",
    "camera",
    "react-native-camera"
  ],
  "repository": "https://github.com/react-native-camera/react-native-camera-v4",
  "author": "Marco Scabbiolo <scabbiolo.marco@gmail.com> (https://github.com/react-native-camera)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/react-native-camera/react-native-camera-v4/issues"
  },
  "homepage": "https://github.com/react-native-camera/react-native-camera-v4#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
